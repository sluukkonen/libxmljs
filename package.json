{
  "name": "libxmljs2",
  "author": "marudor",
  "contributors": [
    "Jeff Smick",
    "Marco Rogers"
  ],
  "binary": {
    "module_name": "xmljs",
    "module_path": "./build/Release/",
    "host": "https://github.com",
    "remote_path": "./marudor/libxmljs2/releases/download/v{version}/",
    "package_name": "{node_abi}-{platform}-{arch}-{libc}.tar.gz"
  },
  "description": "libxml bindings for v8 javascript engine",
  "version": "0.24.0",
  "scripts": {
    "build": "node-pre-gyp install --build-from-source",
    "install": "node-pre-gyp install --fallback-to-build --loglevel http",
    "test": "node --expose_gc ./node_modules/jest/bin/jest.js",
    "lint": "eslint index.js test",
    "tsd": "tsd"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/marudor/libxmljs2.git"
  },
  "bugs": {
    "url": "http://github.com/marudor/libxmljs2/issues"
  },
  "main": "./index",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "bindings": "~1.5.0",
    "nan": "~2.14.0",
    "node-pre-gyp": "~0.14.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-marudor": "^7.2.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.4.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "eslint-plugin-testing-library": "^1.3.4",
    "github-release-cli": "^1.3.1",
    "jest": "^24.9.0",
    "jest-watch-typeahead": "^0.4.2",
    "prettier": "^1.19.1",
    "tsd": "^0.11.0"
  }
}
